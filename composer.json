{
    "name": "alec-lefors/openid-connect-permissions",
    "description": "Set user role and allow access from OpenID user claims.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/alec-lefors/openid-connect-permissions/",
    "authors": [
      {
        "name": "Alec LeFors",
        "email": "alec@lefors.me"
      }
    ],
    "support": {
      "issues": "https://github.com/alec-lefors/openid-connect-permissions/issues"
    },
    "config": {
      "preferred-install": "dist",
      "platform": {
          "php": "8.2"
      },
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      }
    ],
    "require": {
      "php": "8.2",
      "composer/installers": "^1.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "spatie/ray": "^1.29.0"
    },
    "autoload": {
		"psr-4": {
		}
	},
    "minimum-stability": "stable"
  }
